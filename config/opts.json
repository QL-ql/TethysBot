{
    "identity": {
        "identityList": {
            "reg": "^#(身份组列出|列出身份组)$",
            "fnc": "identityList",
            "type": ["GUILD"],
            "describe": "列出身份组"
        },
        "identitySet": {
            "reg": "^#(设置身份组|身份组设置)",
            "fnc": "identitySet",
            "type": ["GUILD"],
            "describe": "身份组设置"
        },
        "identityInfo": {
            "reg": "^#(设置信息|信息设置)",
            "fnc": "identityInfo",
            "type": ["GUILD"],
            "describe": "表情分配设置"
        }
    },
    "another": {
        "getRandomNums": {
            "reg": "^#获取随机数\\s?(\\d+)\\s(\\d+)\\s(\\d+)$",
            "fnc": "getRandomNums",
            "type": ["GUILD", "DIRECT"],
            "describe": "获取随机数xxx yyy zzz"
        }
    },
    "scoreboard": {
        "scoreboardQuery": {
            "reg": "^#(查询点数|点数查询)$",
            "fnc": "scoreboardQuery",
            "type": ["GUILD", "DIRECT"],
            "permission": "anyone",
            "describe": "查询当前点数"
        },
        "scoreboardRanking": {
            "reg": "^#点数排名$",
            "fnc": "scoreboardRanking",
            "type": ["GUILD", "DIRECT"],
            "permission": "anyone",
            "describe": "列出点数排名"
        },
        "scoreboardChange": {
            "reg": "^#(添加|扣除)(\\d+)点数",
            "fnc": "scoreboardChange",
            "type": ["GUILD"],
            "describe": "操作某人点数"
        },
        "scoreboardSetAnswer": {
            "reg": "^#答案(.+)点数(\\d+)人数(\\d+)$",
            "fnc": "scoreboardSetAnswer",
            "type": ["DIRECT"],
            "describe": "根据答案分配点数"
        },
        "scoreboardChangeWithIdentity": {
            "reg": "^#(添加|扣除)(.+)身份组(\\d+)(点数)?$",
            "fnc": "scoreboardChangeWithIdentity",
            "type": ["GUILD"],
            "describe": "(暂无法使用)操作某身份组点数"
        },
        "scoreboardAnswer": {
            "reg": ".*",
            "fnc": "scoreboardAnswer",
            "type": ["DIRECT"],
            "permission": "anyone",
            "describe": "根据答案获取点数"
        }
    }
}
